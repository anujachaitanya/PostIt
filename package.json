{
  "name": "PostIt",
  "version": "1.0.0",
  "description": "This is a content sharing website where you can share your post with others",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "export db=postitDB.db; export knex_development=./database/postitDB.db; export knex_env=development; node server.js",
    "test": " rm -f database/testDB.db;export db=testDB.db; npm run setupKnex; export knex_env=test;./shell_scripts/setTestDb.sh database/testDB.db; nyc mocha;",
    "setupKnex": "export knex_test=./database/testDb.db;touch $knex_test; knex migrate:latest --env test;",
    "lint": "eslint . --ext .js",
    "preCommit": "npm test && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shankarbyageli/PostIt.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/shankarbyageli/PostIt/issues"
  },
  "homepage": "https://github.com/shankarbyageli/PostIt#readme",
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^8.0.1",
    "nyc": "^15.1.0",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "cookie-parser": "^1.4.5",
    "eslint": "^7.5.0",
    "express": "^5.0.0-alpha.8",
    "express-fileupload": "^1.1.8",
    "knex": "^0.21.4",
    "moment": "^2.27.0",
    "morgan": "^1.10.0",
    "pug": "^3.0.0",
    "sinon": "^9.0.2",
    "sqlite3": "^5.0.0"
  }
}
